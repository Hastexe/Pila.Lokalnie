@model List<MajsterFinale.Models.ADVERTS>

@{

    foreach (var item in Model)

    {

        <tr href="@Url.Action("Details", "Ads", new { id = @item.ID })" class="btn btn-xs btn-green tooltips" data-placement="top" data-original-title="View List">
            class="fa fa-eye">

            <td>@item.ID</td>

            <td>@item.USER_ID</td>

            <td>@item.TITLE</td>

            <td>@item.PRICE</td>

            <td>@item.CATEGORY</td>
        </tr>
    }

}